{
  "description": "Deletes the ads to all webpages matching gmail.com",
  "manifest_version": 1,
  "name": "AdFreeGmail",
  "version": "0.1",
  "homepage_url": "https://github.com/MariosGeorgiou/AdFreeGmail",
  
  "icons": {
    "48": "icon-48.png",
    "96": "icon-96.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.gmail.com/*"],
      "js": ["killGmailAds.js"]
    }
  ]
}
